{
  "restartable": "rs",
  "ignore": [
    ".git",
    "node_modules/**/node_modules"
  ],
  "verbose": true,
  "execMap": {
    "js": "node --harmony"
  },
  "error_file": "./logs/stderr.log",
  "out_file": "./logs/stdout.log",
  "trace_file": "./logs/stdtrace.log",
  "log_date_format": "YYYY-MM-DD HH:mm Z",
  "merge_logs": true,
  "env": {
    "NODE_ENV": "development",
    "DEBUG": "*,-nodemon:*,-nodemon,-knex:pool"
  },
  "ext": "js json"
}
